<?xml version='1.0' encoding='utf-8'?>
<widget
  id="<%= id %>"
  version="<%= version %>"
  xmlns="http://www.w3.org/ns/widgets"
  xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name><%= name %></name>
  <description><%= description %></description>
  <author
    email="<%= author.email %>"
    href="<%= author.url %>"><%= author.name %></author>
  <content src="index.html" />
  <access origin="*" />
</widget>
